{
    "data": {
        "url": "/benchmarkresults/csvreader.csv"
    },
    "transform": [
        {
            "filter": "datum.attempt == 'first' && datum.cols==20"
        },
        {
            "calculate": "datum.withna=='true' ? 'With NA values' : 'Without NA values'",
            "as": "withna_verbal"
        }
    ],
    "facet": {
        "column": {
            "field": "rows",
            "type": "ordinal"
        },
        "row": {
            "field": "withna_verbal",
            "type": "nominal",
            "title": ""
        }
    },
    "title": "Results for 20 columns",
    "resolve": {
        "scale": {
            "x": "independent"
        }
    },
    "background": "white",
    "spec": {
        "facet": {
            "row": {
                "field": "file",
                "type": "nominal"
            }
        },
        "spec": {
            "layer": [
                {
                    "encoding": {
                        "x": {
                            "field": "timing",
                            "title": "seconds",
                            "aggregate": "min",
                            "type": "quantitative"
                        },
                        "color": {
                            "field": "package"
                        }
                    },
                    "mark": "bar"
                },
                {
                    "encoding": {
                        "x2": {
                            "field": "timing",
                            "aggregate": "max",
                            "type": "quantitative"
                        },
                        "x": {
                            "field": "timing",
                            "aggregate": "min",
                            "type": "quantitative"
                        }
                    },
                    "mark": "rule"
                },
                {
                    "encoding": {
                        "x": {
                            "field": "timing"
                        }
                    },
                    "mark": "tick"
                }
            ],
            "encoding": {
                "y": {
                    "field": "package",
                    "title": "",
                    "type": "nominal"
                }
            }
        },
        "resolve": {
            "scale": {
                "x": "shared"
            }
        }
    }
}
