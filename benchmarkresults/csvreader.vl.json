{
    "data": {
        "url": "/benchmarkresults/csvreader.csv"
    },
    "transform": [
        {
            "filter": "datum.attempt == 'first'"
        },
        {
            "calculate": "datum.withna=='true' ? 'With NA values' : 'Without NA values'",
            "as": "withna_verbal"
        }
    ],
    "facet": {
        "row": {
            "field": "withna_verbal",
            "type": "nominal",
            "header": {
                "title": "With NA values"
            }
        }
    },
    "spec": {
        "facet": {
            "row": {
                "field": "cols",
                "type": "ordinal",
                "header": {
                    "title": "# of columns"
                }
            }
        },
        "spec": {
            "facet": {
                "row": {
                    "field": "file",
                    "type": "nominal",
                    "header": {
                        "title": "Input filename"
                    }
                }
            },
            "spec": {
                "facet": {
                    "column": {
                        "field": "rows",
                        "type": "ordinal"
                    }
                },
                "resolve": {
                    "scale": {
                        "x": "independent"
                    }
                },
                "spec": {
                    "layer": [
                        {
                            "encoding": {
                                "x": {
                                    "field": "timing",
                                    "title": "seconds",
                                    "aggregate": "min",
                                    "type": "quantitative"
                                },
                                "color": {
                                    "field": "package"
                                }
                            },
                            "mark": "bar"
                        },
                        {
                            "encoding": {
                                "x2": {
                                    "field": "timing",
                                    "aggregate": "max",
                                    "type": "quantitative"
                                },
                                "x": {
                                    "field": "timing",
                                    "aggregate": "min",
                                    "type": "quantitative"
                                }
                            },
                            "mark": "rule"
                        },
                        {
                            "encoding": {
                                "x": {
                                    "field": "timing"
                                }
                            },
                            "mark": "tick"
                        }
                    ],
                    "encoding": {
                        "y": {
                            "field": "package",
                            "title": "",
                            "type": "nominal"
                        }
                    }
                }
            }
        }
    },
    "background": "white",
    "config": {}
}
